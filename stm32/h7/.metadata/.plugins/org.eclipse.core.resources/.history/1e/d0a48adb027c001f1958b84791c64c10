#include "stm32h7xx.h"

void init_usart1_clk(void)
{
	RCC->AHB4ENR |= (1<<0);
	RCC->APB2ENR |= (1<<4);
}

void init_usart1_pin(void)
{
	GPIOA->AFR[1] &= ~(0xf<<4);
	GPIOA->AFR[1] |= (7<<4);
	GPIOA->AFR[1] &= ~(0xf<<8);
	GPIOA->AFR[1] |= (7<<8);
}

void init_usart(void)
{
	USART1->BRR = 0x364

}




