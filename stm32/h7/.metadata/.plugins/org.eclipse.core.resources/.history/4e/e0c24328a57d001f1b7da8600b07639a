#include "stm32h7xx.h"

void init_mac_clk(void)
{
	RCC->APB4ENR |= (1<<1);
	RCC->AHB1ENR |= (1<<17)|(1<<16)|(1<<15);

}


void init_mac_pin(void)
{

}


void init_mac(void)
{
	SYSCFG->PMCR &= ~(0b111<<21);
	SYSCFG->PMCR |= (0b100<<21);

	ETH->DMAMR |= (1<<0);
	while(!(ETH->DMAMR & (1<<0)));

	ETH->MACMDIOAR |= (0b111<<)


}
